# Database
DATABASE_URL="postgresql://username:password@localhost:5432/ofsat_app"

# JWT
JWT_SECRET="your-super-secret-jwt-key-here"
JWT_EXPIRES_IN="24h"

# S3/MinIO Configuration
S3_ENDPOINT="http://localhost:9000"  # MinIO endpoint or AWS S3
S3_REGION="us-east-1"
S3_ACCESS_KEY="minioadmin"           # MinIO access key or AWS access key
S3_SECRET_KEY="minioadmin"           # MinIO secret key or AWS secret key
S3_BUCKET="ofsat-uploads"            # Bucket name for file uploads

# Server
PORT=3001
NODE_ENV="development"

# CORS
ALLOWED_ORIGINS="http://localhost:5173,http://localhost:3000"